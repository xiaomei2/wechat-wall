button[type='primary'] {
  background-color: #DBA871;
}
button[type='primary'][plain] {
  background-color: #ffffff;
  border: 2rpx solid #DBA871;
  color: #DBA871;
}
button[type='primary'].button-hover {
  background-color: rgba(219, 168, 113, 0.6);
}
button[type='default'] {
  color: #666 !important;
  border: 2rpx solid #ccc !important;
  background-color: #ffffff;
}
button[type='info'] {
  background-color: #343434;
  color: #ffffff !important;
}
